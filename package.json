{
  "name": "nuxt-feature-flags-module",
  "version": "2025.6.1",
  "description": "Lightweight, environment-based feature flag system for Nuxt 3 â€” made for developers who need dynamic feature control in routes, components, and APIs.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicokempe/nuxt-feature-flags-module.git"
  },
  "author": {
    "name": "Nico Kempe",
    "email": "kontakt@nicokempe.de",
